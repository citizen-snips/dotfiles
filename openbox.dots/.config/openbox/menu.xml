  <menu id="Screenshot" icon="/usr/share/icons/AwOken/clear/24x24/categories/plugin-screenshot.png" label="Take Screenshot">
      <item label="Now">
         <action name="Execute">
            <execute>
               scrot '%Y-%m-%d--%s_$wx$h_scrot.png' -e 'mv $f ~/ &amp; gpicview ~/$f'
            </execute>
         </action>
      </item>
      <item label="In 5 Seconds...">
         <action name="Execute">
            <execute>
               scrot -d 5 '%Y-%m-%d--%s_$wx$h_scrot.png' -e 'mv $f ~/ &amp; gpicview ~/$f'
            </execute>
         </action>
      </item>
      <item label="In 10 Seconds...">
         <action name="Execute">
            <execute>
               scrot -d 10 '%Y-%m-%d--%s_$wx$h_scrot.png' -e 'mv $f ~/ &amp; gpicview ~/$f'
            </execute>
         </action>
      </item>
      <item label="Selected Area... (click &amp; drag mouse)">
         <action name="Execute">
            <execute>
               scrot -s '%Y-%m-%d--%s_$wx$h_scrot.png' -e 'mv $f ~/ &amp; shotwell ~/$f'
            </execute>
         </action>
      </item>
  </menu>
